  "require": {
    "php": ">=7.1",
    "laravel/framework": "^5.5|^5.6|^5.7"
  },
  "autoload": {
    "psr-4": {
      "Sureyee\\ActionLog\\" : "src"
    }
  },
  "extra": {
    "laravel": {
      "providers" : [
        "Sureyee\\ActionLog\\ActionLogServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}