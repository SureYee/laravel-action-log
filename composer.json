{
  "name": "sureyee/laravel-action-log",
  "description": "基于ORM模型事件的操作记录",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "yixu",
      "email": "yixu@sureyee.com"
    }
